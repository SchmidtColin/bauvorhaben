<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
</head>
<body>
{% block body %}
    <h1>Anfrage</h1>

    <table>
        <tbody>
            <tr>
                <th>Beschreibung</th>
                <td>{{ anfrage.beschreibung }}</td>
            </tr>
            <tr>
                <th>Menge</th>
                <td>{{ anfrage.menge }}</td>
            </tr>
        </tbody>
    </table>

    {{ form_start(delete_form) }}
    <input style="margin: 20px" class="waves-effect waves-light btn" type="submit" value="Delete">
    {{ form_end(delete_form) }}

    <ul>
        <li>
            <a style="margin: 20px" class="waves-effect waves-light btn" href="{{ path('anfrage_edit', { 'id': anfrage.id }) }}">Edit</a>
        </li>
        <li>
            <a style="margin: 20px" class="waves-effect waves-light btn" href="/../bauvorhaben/web/">Hauptmen√º</a>
        </li>
    </ul>

    <div>

    </div>
{% endblock %}
</body>