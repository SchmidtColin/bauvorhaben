{% block body %}
    <h1>Anfrage</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ anfrage.id }}</td>
            </tr>
            <tr>
                <th>Beschreibung</th>
                <td>{{ anfrage.beschreibung }}</td>
            </tr>
            <tr>
                <th>Menge</th>
                <td>{{ anfrage.menge }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('anfrage_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('anfrage_edit', { 'id': anfrage.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>

    <div>
        <a style="margin: 20px" class="waves-effect waves-light btn" href="/../bauvorhaben/web/">Hauptmen√º</a>
    </div>
{% endblock %}
